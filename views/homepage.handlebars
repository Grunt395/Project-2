<!-- Loop over each object in the projects array -->
<input style="display:none" id="userId"
value="{{user_id}}">

{{#each quiz as |q| }}
<div class="row mb-4 project">

  <div class="col-md-7">
    <p>
      {{q.question}}
    </p>
    <p>
      {{q.answer}}
    </p>
    <p>checked = true</p>
    <p>unchecked = false</p>
    <input type="checkbox" id="answer">

  </div>
</div>
{{/each}}
{{#if logged_in}}
<button id="submitQuiz">Submit</button>
{{/if}}